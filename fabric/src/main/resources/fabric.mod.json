{
  "schemaVersion": 1,
  "id": "hexxyplanes",
  "version": "${version}",
  "name": "Hexxyplanes",
  "description": "A small personal dimension, perfect for all your secret keeping needs",
  "authors": [
    "Real-Septicake"
  ],
  "contact": {
    "homepage": "https://real-septicake.github.io/HexxyPlanes",
    "sources": "https://github.com/Real-Septicake/HexxyPlanes"
  },
  "license": "MIT",
  "icon": "assets/hexxyplanes/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "io.github.real_septicake.hexxyplanes.fabric.FabricHexxyplanes"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "io.github.real_septicake.hexxyplanes.fabric.FabricHexxyplanesClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "io.github.real_septicake.hexxyplanes.fabric.FabricHexxyplanesServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "io.github.real_septicake.hexxyplanes.fabric.datagen.FabricHexxyplanesDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "io.github.real_septicake.hexxyplanes.fabric.FabricHexxyplanesModMenu"
    }],
    "cardinal-components": [
      "io.github.real_septicake.hexxyplanes.fabric.HexxyplanesComponents"
    ]
  },
  "mixins": [
    "hexxyplanes-common.mixins.json",
    "hexxyplanes-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting_dependency}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  },
  "breaks": {
    "hexical": "<2.0.0"
  },
  "custom": {
    "cardinal-components": [
      "hexxyplanes:exit"
    ]
  }
}
